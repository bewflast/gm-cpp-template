{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 25,
        "patch": 0
    },
    "include": ["../architectures/architectures.json"],
    "configurePresets": [
        {
            "hidden": true,
            "name": "clang",
            "cacheVariables": {
				"CMAKE_C_COMPILER": "clang",
				"CMAKE_CXX_COMPILER": "clang"
			}
        },
        {
            "hidden": true,
            "name": "win clang",
            "cacheVariables": {
				"CMAKE_C_COMPILER": "clang-cl.exe",
				"CMAKE_CXX_COMPILER": "clang-cl.exe"
			}
        },
        {
            "hidden": true,
            "name": "clang-x86",
            "inherits": ["arch-x86", "clang"],
            "cacheVariables": {
				"CMAKE_C_FLAGS": "-m32",
				"CMAKE_CXX_FLAGS": "-m32"
			}
        },
        {
            "hidden": true,
            "name": "clang-x86_64",
            "inherits": ["arch-x86_64", "clang"]
        },
        {
            "hidden": true,
            "name": "win clang-x86",
            "inherits": ["win clang"],
            "cacheVariables": {
				"CMAKE_C_FLAGS": "-m32",
				"CMAKE_CXX_FLAGS": "-m32"
			}
        },
        {
            "hidden": true,
            "name": "win clang-x86_64",
            "inherits": ["win clang"]
        }
    ]
}